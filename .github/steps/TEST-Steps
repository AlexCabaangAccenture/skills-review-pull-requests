            "ReceivedQuantity": "integer",
            "PurchaseOrderLine": "string"
        },
        "Details": [
            {
                "ProductID": "string",
                "ProductDesc": "string",
                "UnitPrice": "number",
                "LineTotal": "number",
                "OrderQty": "integer",
                "LineTax": "number",
                "ReceivedQty": "integer",
                "GLAccount": "string",
                "CostCenter": "string",
                "DueDate": "string (ISO 8601 date)",
                "POdetailKey": "string",
                "POHeaderKey": "string",
